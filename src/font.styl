@import "variables"

@font-face
    font-family 'icons'
    src url(icomoon-font-path + '/icons.eot?it4h45')
    src url(icomoon-font-path + '/icons.eot?it4h45#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icons.ttf?it4h45') format('truetype'), url(icomoon-font-path + '/icons.woff?it4h45') format('woff'), url(icomoon-font-path + '/icons.svg?it4h45#icons') format('svg')
    font-weight normal
    font-style normal

[class^="icon-"], [class*=" icon-"]
    font-family 'icons' !important
    speak none
    font-style normal
    font-weight normal
    font-variant normal
    text-transform none
    line-height 1

    /* Better Font Rendering =========== */
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

